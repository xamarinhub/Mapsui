<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mapsui.ArcGIS</id>
    <version>$version$</version>
    <title>Mapsui</title>
    <authors>Paul den Dulk</authors>
    <owners>Paul den Dulk</owners>
    <projectUrl>https://github.com/Mapsui/Mapsui</projectUrl>
    <license type="file">LICENSE</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Mapsui components to access an ArcGIS dynamic layer and image layer.
    </description>
    <tags>map maps mapping geo gis ArcGIS esri tiles tiling</tags>
    <language>en-US</language>
    <dependencies>
      <!-- [ or ] is including, ( or ) is excluding -->
      <group targetFramework="netstandard2.0">
        <dependency id="Mapsui" version="$version$"/>
        <dependency id="Newtonsoft.json" version="[13.0.1,)"/>
      </group>
      <group targetFramework="net6.0">
	    <dependency id="Mapsui" version="$version$"/>
	    <dependency id="Newtonsoft.json" version="[13.0.1,)"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\LICENSE" target="LICENSE" />
    <file src="..\Mapsui.ArcGIS\bin\Release\netstandard2.0\Mapsui.ArcGIS.dll" target="lib\netstandard2.0\Mapsui.ArcGIS.dll" />
    <file src="..\Mapsui.ArcGIS\bin\Release\net6.0\Mapsui.ArcGIS.dll" target="lib\net6.0\Mapsui.ArcGIS.dll" />
  </files>
</package>
